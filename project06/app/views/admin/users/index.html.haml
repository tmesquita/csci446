%h1 Listing users
%p
  #notice= notice
%p
  = will_paginate @users
%table
  %tr
    %th Name
    %th Username
    %th Last Login
    %th Number of Games
  - @users.each do |user|
    %tr
      %td= link_to user.full_name, edit_admin_user_path(user)
      %td= user.username
      %td= user.last_login_at
      %td= user.games.count
%br/
= link_to 'New User', new_admin_user_path
