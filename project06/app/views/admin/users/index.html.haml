%h1 Listing users
%p
  #notice= notice
%table
  %tr
    %th Username
    %th Email
    %th First Name
    %th Last Name
    %th
    %th
    %th
  - @users.each do |user|
    %tr
      %td= user.username
      %td= user.email
      %td= user.first_name
      %td= user.last_name
      %td= link_to 'Show', admin_user_path(user)
      %td= link_to 'Edit', edit_admin_user_path(user)
      %td= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete
%br/
= link_to 'New User', new_admin_user_path
